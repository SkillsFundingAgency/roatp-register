<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Sfa.Roatp.Register.Web.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="Sfa.Roatp.Register.Web" vmsize="Standard_D2_v2">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
          <Binding name="Endpoint2" endpointName="Endpoint2" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="EnvironmentName" />
      <Setting name="RoatpProviderIndexAlias" />
      <Setting name="ElasticServerUrls" />
      <Setting name="FeatureToggle.RobotsAllowFeature" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="https" port="443" certificate="WebSslCert" loadBalancer="roatp-ilb" />
      <InputEndpoint name="Endpoint2" protocol="http" port="80" loadBalancer="roatp-ilb" />
    </Endpoints>
    <Certificates>
      <Certificate name="WebSslCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>