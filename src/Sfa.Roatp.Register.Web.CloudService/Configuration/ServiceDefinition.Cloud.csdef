<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Sfa.Roatp.Register.Web.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="Sfa.Roatp.Register.Web" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
          <Binding name="Endpoint2" endpointName="Endpoint2" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="EnvironmentName" />
      <Setting name="RoatpProviderIndexAlias" />
      <Setting name="ElasticServerUrls" />
      <Setting name="FeatureToggle.RobotsAllowFeature" />
      <Setting name="InstrumentationKey" />
      <Setting name="LoggingRedisConnectionString" />
      <Setting name="ElasticSearch.Username" />
      <Setting name="ElasticSearch.Password" />
      <Setting name="EnableES5" />
      <Setting name="FeatureToggle.IgnoreSslCertificateFeature" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
      <InputEndpoint name="Endpoint2" protocol="https" port="443" certificate="WebSslCert" />
    </Endpoints>
    <Certificates>
      <Certificate name="WebSslCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>